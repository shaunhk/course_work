<h1>Index Page</h1>
<a href="/hogwarts/create">Add New Student</a>
<a href="/hogwarts/houses">All Houses</a>
<br>

<% for student in @students %>
  <p>Name: <%= student.first_name %> <%= student.second_name %></p>
  <p>Age: <%= student.age %></p>
  <p>House: <%= student.house_id %></p>
  <a href="/hogwarts/<%=student.id%>/houses">House Detail</a>
  <hr>
<% end %>
